#include<stdio.h>
#include<math.h>
#include<stdlib.h>
int main()
{
    int t,v;
    long long int a,s,b,c,d,e,n;
    scanf("%d",&t);
    for(int i=1;i<=t;i++)
    {
        scanf("%lld",&n);
        scanf("%lld",&a);
        
        s=a+(2*((long long)pow(10,n)));
        printf("%lld\n",s);
        fflush(stdout);
        
        scanf("%lld",&b);
        
        c=s-a-b-((long long)(pow(10,n)));
        printf("%lld\n",c);
        fflush(stdout);
        
        scanf("%lld",&d);
        
        
        e=s-a-b-c-d;
        printf("%lld\n",e);
        fflush(stdout);
        
        scanf("%d",&v);
        if(v==-1)
        {
            break;
        }
    }
}
